{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile"
  },
  "start": "/home/warrior/start.py",
  "variables": {
    "DOWNLOADER": {
      "description": "Your nickname (shown on leaderboards)",
      "value": "your_nickname_here"
    },
    "SELECTED_PROJECT": {
      "description": "Project to run (use 'auto' for ArchiveTeam's Choice)",
      "value": "auto"
    },
    "CONCURRENT_ITEMS": {
      "description": "Max concurrent items (start low: 2-3 on Railway)",
      "value": "3"
    }
  }
}
